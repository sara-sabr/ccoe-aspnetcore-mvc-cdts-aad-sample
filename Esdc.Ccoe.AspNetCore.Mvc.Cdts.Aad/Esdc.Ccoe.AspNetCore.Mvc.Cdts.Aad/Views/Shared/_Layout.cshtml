<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>ASP.NET Core CDTS AAD Sample</title>

    <script type="text/javascript" src="https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_26/cdts/compiled/soyutils.js"></script>
    <script type="text/javascript" src="https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_26/cdts/compiled/wet-en.js"></script>
    <script type="text/javascript">document.write(wet.builder.refTop({"cdnEnv":"prod", "isApplication":true}));</script>

    <link rel="icon" href="https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_26/assets/favicon.ico" type="image/x-icon" />
    <link rel="stylesheet" href="https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_26/css/theme.min.css" />
    <link rel="stylesheet" href="https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_26/cdts/gctheme.css" />

    <link rel="stylesheet" href="~/css/site.css" type="text/css" />
</head>

<body vocab="http://schema.org" typeof="WebPage">
    <div id="def-top">
    <ul id="wb-tphp">
        <li class="wb-slc"><a class="wb-sl" href="#wb-cont">Skip to main content</a></li>
        <li class="wb-slc visible-sm visible-md visible-lg"><a class="wb-sl" href="#wb-info">Skip to "About this site"</a></li>
    </ul>
    <header role="banner">
        <div id="wb-bnr">
            <div id="app-brand">
                <div class="container">
                    <div class="row">
                        <section class="col-lg-2 col-md-6 col-sm-9 col-xs-8">
                            <div class="app-name">
                                <a href="https://intranet.canada.ca/index-eng.asp"><span><span class="bold-gc">GC</span>intranet</span></a>
                            </div>
                        </section>
                        <section id="wb-gctools" class="col-lg-6 visible-lg">
                            <div class="pull-left tool-link">
                                <a href="https://gcconnex.gc.ca/splash/">
                                    <span class="bold-gc">GC</span>connex
                                </a>
                            </div>
                            <div class="pull-left tool-link">
                                <a href="http://www.gcpedia.gc.ca/wiki/?setlang=en">
                                    <span class="bold-gc">GC</span>pedia
                                </a>
                            </div>
                            <div class="pull-left tool-link">
                                <a href="http://gcdirectory-gcannuaire.gc.ca/en/GCD/?pgid=002">
                                    <span class="bold-gc">GC</span>directory
                                </a>
                            </div>
                            <div class="pull-left tool-link">
                                <a href="https://gccollab.ca/splash/">
                                    <span class="bold-gc">GC</span>collab
                                </a>
                            </div>
                        </section>
                        <section id="wb-lng" class="col-lg-4 col-md-5 hidden-sm hidden-xs text-right">
                            <h2 class="wb-inv">Language selection</h2>
                            <div class="row">
                                <div class="col-md-12">
                                    <ul class="list-inline margin-bottom-none gcbarlng">
                                        @*<li><a lang="fr" href="#">Français</a></li>*@
@*                                        @foreach (var languageLink in LanguageLinks)
                                        {
                                            <li><a lang="@languageLink.Language" href="@languageLink.Href">@languageLink.Text</a></li>
                                        }*@
                                    </ul>
                                </div>
                            </div>
                        </section>
                        <section class="wb-mb-links col-xs-4 col-sm-3 visible-sm visible-xs" id="wb-glb-mn">
                            
                            <h2>Menus</h2>
                            <ul class="list-inline text-right chvrn">
                                <li>
                                    <a href="#mb-pnl" title="Menus" aria-controls="mb-pnl" class="overlay-lnk" role="button">
                                        <span class="glyphicon glyphicon-th-list">
                                            <span class="wb-inv">Menus</span>
                                        </span>
                                    </a>
                                </li>
                            </ul>
                            <div id="mb-pnl"></div>
                        </section>
                    </div>
                    <div id="tools-sm" class="row visible-md">
                        <section id="tool-container-sm" class="col-md-12 col-sm-12">
                            <div class="pull-left tool-link"><a href="https://gcconnex.gc.ca"><span class="bold-gc">GC</span>connex</a></div>
                            <div class="pull-left tool-link"><a href="http://www.gcpedia.gc.ca/wiki/?setlang=en"><span class="bold-gc">GC</span>pedia</a></div>
                            <div class="pull-left tool-link"><a href="http://gcdirectory-gcannuaire.gc.ca/en/GCD/?pgid=002"><span class="bold-gc">GC</span>directory</a></div>
                            <div class="pull-left tool-link"><a href="https://gccollab.ca/splash/"><span class="bold-gc">GC</span>collab</a></div>
                        </section>
                    </div>
                </div>
            </div>
        </div>
        <div id="gcappbanner">
            <div class="container">
                    <div id="wb-sttl" class="col-md-10">
                        <a asp-area="" asp-controller="Home" asp-action="Index">
                            <span>hello</span>
                        </a>
                    </div>
                    <div class="text-right col-md-2 box-page-header-icon">
                    </div>
                </div>
            </div>
        </div>
    </header>
</div>

<main role="main" property="mainContentOfPage" class="container">
    @RenderBody()

    <div id="def-preFooter">
    <div class="row pagedetails">
        <div class="col-sm-5 col-xs-12 datemod">
            <dl id="wb-dtmd">
                <dt>Date modified:&#32;</dt>
                <dd><time property="dateModified">2016-05-04</time></dd>
            </dl>
        </div>
    </div>
    </div>
    <script type="text/javascript">
    document.write(wet.builder.refFooter({
        cdnEnv: "prod"
    }));
    </script>
</main>

<div id="def-footer">
    <footer role="contentinfo" id="wb-info">
        <div class="brand">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-md-8">
                        <a href="https://www.canada.ca/en.html"><object type="image/svg+xml" tabindex="-1" data='https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_26/assets/sig-blk-en.svg'></object><span class="wb-inv">Government of Canada</span></a>
                    </div>
                    <div class="col-xs-6 visible-sm visible-xs tofpg">
                        <a href="#wb-cont">Top of Page<span class="glyphicon glyphicon-chevron-up"></span></a>
                    </div>
                    <div class="col-xs-6 col-md-4 text-right">
                        <object type="image/svg+xml" tabindex="-1" role="img" data="https://ssl-templates.services.gc.ca/app/cls/WET/gcintranet/v4_0_26/assets/wmms-blk.svg" aria-label="Symbol of the Government of Canada"></object>
                    </div>
                </div>
            </div>
        </div>
    </footer>
</div>
<!-- Write closure template -->
<script type="text/javascript">
    var defFooter = document.getElementById("def-footer");
    defFooter.outerHTML = wet.builder.appFooter({
        cdnEnv: "prod",
        showFeatures: false
    });
</script>
<!-- Write closure template -->
<script type="text/javascript">
    document.write(wet.builder.refFooter({
        cdnEnv: "prod"
    }));
</script>

<script type="text/javascript" src="~/js/site.js"></script>
@*<script>
    $(function(){
        ReadySite.init();
    });
</script>*@
@RenderSection("scripts", required: false)
</body>

